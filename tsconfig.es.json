{
    "compilerOptions": {
        "strictNullChecks": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "jsx": "react",
        "declaration": true,
        "noUnusedParameters": true,
        "noUnusedLocals": true,
        "noImplicitAny": true,
        "resolveJsonModule": true,
        "target": "es6",
        "lib": ["dom", "es2017"]
    },
    "exclude": ["node_modules", "lib", "es"]
}
